<Window x:Class="Airob.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Airob"
        xmlns:view="clr-namespace:Airob.View"
        mc:Ignorable="d"
        DataContext="{Binding Main, Source={StaticResource Locator}}"
        Title="MainWindow" Height="400" Width="600">
    <DockPanel>
        <view:Toolbars x:Name="ToolbarsComponent" DockPanel.Dock="Top" />
        <view:Tools x:Name="ToolsComponent" DockPanel.Dock="Left" />
        <view:CanvasView x:Name="CanvasComponent" DockPanel.Dock="Left" />
        
    </DockPanel>
</Window>
